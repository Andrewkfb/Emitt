<header>
  <nav>
    <div class="nav-wrapper">
      <a href="#" class="brand-logo">     Emitt</a>
      <ul id="nav-mobile" class="right hide-on-med-and-down">
        <li><a href="#">Sign Up</a></li>
        <li><a href="#">Log In</a></li>
      </ul>
    </div>
  </nav>
</header>
<br>
<div class="container">
<h1>Create an Account</h1>

  <div class="row">
    <form class="col s12">
      <div class="row">
        <div class="input-field col s6">
          <input placeholder="Chloe Smith" id="name" type="text" class="validate">
          <label for="name">Name</label>
        </div>
      </div>
 

      <div class="row">
        <div class="input-field col s12">
          <input placeholder="csmithandwesson@peekaboo.com"id="email" type="email" class="validate">
          <label for="email">Email</label>
        </div>
      </div>
      

      <div class="row">
        <div class="input-field col s12">
          <input placeholder="Shhhhh, it's a secret :)"id="password" type="password" class="validate">
          <label for="password">Password</label>
        </div>
      </div>

  <button class="btn waves-effect waves-light" type="submit" name="action">Create an Account
    <i class="mdi-content-send right"></i>
  </button>

    </form>
  </div>

  <%= link_to 'Back', users_path %>
  
</div>


